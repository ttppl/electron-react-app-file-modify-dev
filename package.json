{
  "name": "electron-react-app",
  "version": "0.1.0",
  "private": true,
  "main": "main.js",
  "homepage": "./",
  "author": "ttppl",
  "description": "file-modify",
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start-react": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "startElectron": "set NODE_ENV=development&&chcp 65001 && electron .",
    "dev": "concurrently \"npm run start-react\" \"npm run startElectron\"",
    "package-app": "electron-packager . electron-react-app --platform=win32 --arch=x64 --download.mirrorOptions.mirror=https://npm.taobao.org/mirrors/electron/  --overwrite --ignore=node_modules --electron-version=11.1.1",
    "packag-all": "react-scripts build && electron-packager . electron-react-app --platform=win32 --arch=x64 --download.mirrorOptions.mirror=https://npm.taobao.org/mirrors/electron/  --overwrite --ignore=node_modules --electron-version=11.1.1"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "extraResources": [
    "nodeFunctions/*",
    "config.json"
  ],
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^7.3.0",
    "electron": "^20.0.1",
    "electron-packager": "^16.0.0",
    "node-sass": "^7.0.1"
  }
}
